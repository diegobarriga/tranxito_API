{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "postgresDB"
  },
  "AccessToken": {
    "dataSource": "postgresDB",
    "public": false
  },
  "ACL": {
    "dataSource": "postgresDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "postgresDB",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "postgresDB",
    "public": false
  },
  "Person": {
    "dataSource": "postgresDB",
    "public": true
  },
  "Driver": {
    "dataSource": "postgresDB",
    "public": true
  },
  "MotorCarrier": {
    "dataSource": "postgresDB",
    "public": true
  },
  "Vehicle": {
    "dataSource": "postgresDB",
    "public": true
  },
  "Event": {
    "dataSource": "mongoDB",
    "public": true
  }
}
