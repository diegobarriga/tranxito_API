{
  "postgresDB": {
    "host": "${E2E_relational_host}",
    "port": 5432,
    "database": "${E2E_relational_db}",
    "password": "${E2E_relational_pass}",
    "name": "postgresDB",
    "user": "${E2E_relational_user}",
    "ssl": true,
    "connector": "postgresql",
    "poolIdleTimeout": 1200000
  },
  "mongoDB": {
    "url": "${MONGODB_URI}",
    "name": "mongoDB",
    "connector": "mongodb"
  },
  "azureStorage": {
    "name": "azureStorage",
    "connector": "loopback-component-storage",
    "provider": "azure",
    "storageAccount": "",
    "storageAccessKey": ""
  },
    "container": {
    "name": "container",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "tmp"
  }
}