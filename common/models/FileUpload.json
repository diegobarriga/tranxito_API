{
  "name": "FileUpload",
  "plural": "file-uploads",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "promisify": true
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "generated": true,
      "index": {
        "unique": true
      }
    },
    "date": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "fileType": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "errors": {
      "type": "hasMany",
      "model": "FileUploadError",
      "foreignKey": "fileUploadId"
    },
    "person": {
      "type": "belongsTo",
      "model": "Person",
      "foreignKey": "personId"
    }
  },
  "acls": [],
  "methods": {}
}
