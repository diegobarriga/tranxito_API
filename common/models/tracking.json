{
  "name": "Tracking",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "indexes": {
    "timestampVehicleIndex": {
      "keys": {
        "timestamp": 1,
        "vehicleId": -1
      },
      "options": {
        "unique": true
      }
    },
    "timestampDriverIndex": {
      "keys": {
        "timestamp": 1,
        "driverId": -1
      },
      "options": {
        "unique": true
      }
    }
  },
  "properties": {
    "coordinates": {
      "type": "geopoint",
      "required": true
    },
    "speed": {
      "type": "number",
      "required": true
    },
    "timestamp": {
      "type": "date",
      "required": true
    },
    "speed_limit_exceeded": {
      "type": "boolean",
      "required": true
    },
    "drive_time_exceeded": {
      "type": "boolean",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "person": {
      "type": "belongsTo",
      "model": "Person",
      "foreignKey": ""
    },
    "vehicle": {
      "type": "belongsTo",
      "model": "Vehicle",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "A",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__vehicle"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__person"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "S",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "S",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "S",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "S",
      "permission": "ALLOW",
      "property": "__get__vehicle"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "S",
      "permission": "ALLOW",
      "property": "__get__person"
    }
  ],
  "methods": {}
}
